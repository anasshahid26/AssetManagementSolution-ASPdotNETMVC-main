        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/Scripts/jquery.dataTables.min.js"></script>
        <script src="~/Scripts/dataTables.buttons.min.js"></script>
        <script src="~/Scripts/jszip.min.js"></script>
        <script src="~/Scripts/buttons.html5.min.js"></script>
        <script src="~/Scripts/dataTables.bootstrap.min.js"></script>

        <script type="text/javascript">
            // "<tr class='scanedAssets danger'><td><a id='" + AssetNumber + "' href='#' class='missingBarcode'>" + AssetNumber + "</a></td><td>" + AssetDescription + "</td><td class='AssetStatus'>" + Status + "</td><td class='AssetStatusHide' style='display:none;'>" + Status + "</td><td><input id='chkSelect' class='chk' name='asset[]' type='checkbox' value='" + AssetNumber + "' onclick='ChangeCheckboxLabel(this)'></td></tr>";

            // "<tr class='scanedAssets'><td>" + AssetNumber + "</td><td>" + AssetDescription + "</td><td class='AssetStatus'>" + Status + "</td><td class='AssetStatusHide' style='display:none;'>" + Status + "</td><td><input id='chkSelect' class='chk' name='asset[]' type='checkbox' value='" + AssetNumber + "' checked onclick='ChangeCheckboxLabel(this)'></td></tr>";

            Table = $("#tblData").DataTable({

                data: [],
                columns: [

                     { title: "Asset Number", data: 'AssetNumber', className: 'text-center' },
                     { title: "Asset Description", data: 'AssetDescription', className: 'text-center' },
                     { title: "VerificationStatus", data: 'VerificationStatus', className: 'AssetStatusHide' },
                     { title: "Status", data: 'Status', className: 'AssetStatus' },
                     //{ data: 'Status', className: 'checkbox' },
                    {
                        data: "Status",
                        render: function (data, type, row) {

                            if (data === 'MISSING') {
                                //$row.css({ 'background-color': 'red' });
                                return "<input id='chkSelect' class='chk' name='asset[]' type='checkbox' value='" + data.AssetNumber + "' onclick='ChangeCheckboxLabel(this)'>";//'<input type='checkbox' class='editor-active'>';
                            }
                            else {

                                return "<input id='chkSelect' class='chk' name='asset[]' type='checkbox' value='" + data.AssetNumber + "' checked onclick='ChangeCheckboxLabel(this)'>";//'<input type='checkbox' class='editor-active'>';
                            }
                            return data;
                        }
                    }
                ],
                dom: 'Bfrtip',
                "pageLength": 1000,
                buttons: [{
                    extend: 'excelHtml5',
                    text: 'Export to Excel',
                    exportOptions: {
                        columns: ':visible:not(.not-export-col)'
                    },

                    customize: function (xlsx) {
                        var sheet = xlsx.xl.worksheets['sheet1.xml'];
                        var downrows = 3;
                        var clRow = $('row', sheet);


                        //update Row
                        clRow.each(function () {
                            var attr = $(this).attr('r');
                            var ind = parseInt(attr);
                            ind = ind + downrows;
                            $(this).attr("r", ind);
                        });

                        // Update  row > c
                        $('row c ', sheet).each(function () {
                            var attr = $(this).attr('r');
                            var pre = attr.substring(0, 1);
                            var ind = parseInt(attr.substring(1, attr.length));

                            ind = ind + downrows;
                            $(this).attr("r", pre + ind);
                        });

                        //EXCEL HEADER(1st 2 rows of excel file)
                        function Addrow(index, data) {
                            msg = '<row r="' + index + '">'
                            for (i = 0; i < data.length; i++) {
                                var key = data[i].k;
                                var value = data[i].v;

                                msg += '<c t="inlineStr" r="' + key + index + '" s="42">';
                                msg += '<is>';
                                msg += '<t>' + value + '</t>';
                                msg += '</is>';
                                msg += '</c>';
                            }
                            msg += '</row>';
                            return msg;
                        }

                        var locat = $("#Location option:selected").text();
                        //insert

                        var D = new Date();
                        //  D.format('dd/MM/yyyy hh:mm TT');

                        //insert
                        var r1 = Addrow(1, [{ k: 'A', v: 'Location Name' }, { k: 'B', v: locat }, { k: 'C', v: 'Generated by Matrix FAS' }]);  //Addrow(1, [{ k: 'A', v: 'Matrix Fixed Asset Solution' }]);
                        var r2 = Addrow(2, [{ k: 'A', v: 'Report Name' }, { k: 'B', v: 'Manual Reverification' }, { k: 'C', v: D }]);
                        //var r3 = Addrow(3, [{ k: 'A', v: 'Location Name' }, { k: 'B', v: locat }]);
                        sheet.childNodes[0].childNodes[1].text = r1 + r2 + sheet.childNodes[0].childNodes[1].text;

                        //var r1 = Addrow(1, [{ k: 'A', v: 'Matrix Fixed Asset Solution' }]);
                        //var r2 = Addrow(2, [{ k: 'A', v: 'Report Name' }, { k: 'B', v: 'Asset Search Report' }]);
                        //var r3 = Addrow(3, [{ k: 'A', v: 'Location Name' }, { k: 'B', v: locat }]);
                        //sheet.childNodes[0].childNodes[1].innerHTML = r1 + r2 + r3 + sheet.childNodes[0].childNodes[1].innerHTML;
                    }
                }],
                rowCallback: function (row, data) {
                    if (data.Status == "DISPOSED") {
                        $('td', row).remove();
                    }
                    else if (data.Status == "MISSING") {
                        $('td', row).addClass('danger');
                    }
                    else {
                        //$('td', row).removeclassClass('danger');
                    }

                    var imgLink = data['Image'];
                    var assetNumber = data['AssetNumber'];
                    var imgTag = '<img src="' + baseUrl.appServer + 'Content/img/' + imgLink + '.jpg" id="' + assetNumber + '" data-toggle="modal" data-target="#myModal" class="images" style="width:25px; height:25px;"/>';
                    $('td:eq(11)', row).html(imgTag);
                    return row;
                }
            });


            $('#close').unbind().click(function () {
                $("#alertfailedModal").toggle();
            });


            var role = $('#role').val();
            if (role == "USER" || role == "UADMIN") {

                var userId = $('#userid').val();
                var formData = {
                    type: "post",
                    data: { 'UserID': userId }
                };

                GetUserLocation(formData, function (response) {
                    //if (response.data.length == 1) {
                    //    $('#def').remove();
                    //}
                    $.each(response.data, function (key, value) {
                        $("#Location").append($("<option></option>").val(value.L1LocCode).html(value.L1LocName));
                    });
                });
                setTimeout(function () {
                    var Location1 = $("#Location").find(':selected').val();
                    var formData = {
                        type: "POST",
                        data: { 'L1LocCode': Location1 }
                    };

                    FillLocations();
                    //GetHierarchy(formData);

                    $('#Scan').unbind().click(function () {

                        $('.scanedAssets').remove();
                        $('#dbCount').text('0');
                        $('#scCount').text('0');

                        var Location = $("#Location").find(':selected').val();
                        var AssetSection = $("#AssetSection").find(':selected').text();
                        var AssetFloor = $("#AssetFloor").find(':selected').text();
                        var AssetRoomNo = $("#AssetRoomNo").find(':selected').text();
                        var AssetInfo = [];
                        var foo = $('#AN').val();

                        foo = foo.split('\n');
                        foo = foo.filter(function (entry) { return entry.trim() != ''; });

                        $.each(foo, function (key, value) {
                            AssetInfo.push({
                                AssetNumber: value
                            });
                        });

                        var form = { "L1LocCode": Location, "L2LocName": AssetSection, "L3LocName": AssetRoomNo, "CODELEVEL": AssetFloor, "Assets": AssetInfo };
                        var formData = {
                            type: "POST",
                            data: form
                        };

                        AssetReverification(formData, function (response) {
                            //CreateDataTable(response);
                            //CreateGrid(response);

                            $('#dbCount').text(response.data.Count);
                            $('#scCount').text(foo.length);

                            for (var i = 0; i < response.data.VerifiedAssets.length; i++) {
                                var AssetNumber = response.data.VerifiedAssets[i].AssetNumber;
                                var AssetDescription = response.data.VerifiedAssets[i].AssetDescription;
                                var Status = response.data.VerifiedAssets[i].Status;
                                console.log(response.data.VerifiedAssets);

                                if (Status === "NO NAME") {
