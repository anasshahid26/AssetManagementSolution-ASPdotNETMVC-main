                                        $('#ReplaceButton').unbind().click(function (event) {
                                            event.preventDefault();
                                            var NewBarcode = $('#NEWBARCODE').val();
                                            var form = { "OldBarcode": OldBarcode, "NewBarcode": NewBarcode, "L1LocCode": $("#Location").val() };
