                values = id.split('*');
                Active = values[1];
                CompanyID = values[0];
                if (Active == "true") {
                    $("#example2").show();
                    $('#ADU').show();
                    $('.com').remove();
                    $('.result').remove();
                    var formData = {
                        type: 'POST',
                        data: { 'CompanyID': CompanyID }
                    };
